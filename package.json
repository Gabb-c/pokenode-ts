{
  "name": "pokenode",
  "version": "1.0.0",
  "description": "The PokéAPI Node.js wrapper with built-in types.",
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "build": "tsc --diagnostics --pretty",
    "types": "dts-bundle-generator -o ./dist/@types/index.d.ts ./src/index.ts --project ./tsconfig.json --umd-module-name pokenode",
    "build:ci": "yarn build && yarn types",
    "docs": "docsify serve ./docs",
    "lint": "eslint src --ext js,ts",
    "lint:fix": "eslint src --ext js,ts --fix",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "test:watch": "jest --watch --verbose --colors --expand --maxWorkers=25%",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated",
    "test:ci": "jest --ci --runInBand",
    "prepare": "is-ci || husky install"
  },
  "keywords": [
    "pokeapi, PokéAPI, pokenode, Pokenode, pokemon"
  ],
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabb-c/pokenode"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dts-bundle-generator": "^5.9.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21"
  }
}
