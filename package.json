{
  "name": "pokenode-ts",
  "version": "1.19.0",
  "private": false,
  "description": "A lightweight Node.js wrapper for the PokéAPI with built-in types.",
  "keywords": [
    "pokeapi",
    "PokéAPI",
    "pokemon",
    "pokedex"
  ],
  "homepage": "https://pokenode-ts.vercel.app/",
  "bugs": {
    "url": "https://github.com/Gabb-c/pokenode-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabb-c/pokenode-ts"
  },
  "funding": "https://github.com/Gabb-c/pokenode-ts?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs --force",
    "docs:preview": "vitepress preview docs",
    "format": "rome format ./src",
    "format:fix": "rome format --write ./src",
    "lint": "rome check src",
    "lint:fix": "rome check --apply-unsafe ./src",
    "lint:pkg": "sort-package-json",
    "lint:tsc": "tsc --extendedDiagnostics",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pnpm build",
    "size": "pnpm build && size-limit",
    "test:ci": "vitest --silent --run",
    "test:coverage": "vitest run --coverage",
    "test:dev": "vitest -w",
    "test:ui": "vitest --ui --api 9527"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.19.1",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.5",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@swc/core": "^1.3.78",
    "@types/is-ci": "^3.0.0",
    "@types/node": "^20.5.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.2.0",
    "ci-info": "^3.8.0",
    "dotenv": "^16.3.1",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^14.0.0",
    "rome": "^12.1.3",
    "semantic-release": "^21.0.9",
    "size-limit": "^8.2.6",
    "sort-package-json": "^2.5.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitepress": "1.0.0-rc.4",
    "vitest": "^0.34.2",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.2.0"
  },
  "engines": {
    "node": ">=14"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "10 kB"
    }
  ]
}
