{
  "name": "pokenode",
  "version": "1.0.0",
  "description": "The PokéAPI Node.js wrapper with built-in types.",
  "keywords": [
    "pokeapi, PokéAPI, pokenode, Pokenode, pokemon"
  ],
  "homepage": "https://github.com/Gabb-c/pokenode",
  "bugs": {
    "url": "https://github.com/Gabb-c/pokenode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabb-c/pokenode"
  },
  "license": "MIT",
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --diagnostics --pretty",
    "types": "dts-bundle-generator --config dts-generator.json",
    "build:ci": "yarn build && yarn types",
    "docs": "docsify serve ./docs",
    "lint": "eslint src --ext js,ts",
    "lint:fix": "eslint src --ext js,ts --fix",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "lint:ci": "yarn lint:fix && yarn format && yarn sort",
    "sort": "sort-package-json package.json",
    "test:watch": "jest --watch --verbose --colors --expand --maxWorkers=25%",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --verbose --colors --expand --maxWorkers=50% --detectOpenHandles --errorOnDeprecated --bail",
    "test:ci": "jest --ci --runInBand --bail",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.7.3",
    "axios-logger": "^2.5.0",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "pino": "^6.11.3",
    "pino-pretty": "^5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.5",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@types/pino": "^6.3.8",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "dts-bundle-generator": "^5.9.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "sort-package-json": "^1.50.0",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
